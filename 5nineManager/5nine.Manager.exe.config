<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <log4net>
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="%property{LogFileName}"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="10MB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-5p %d %5rms %-22.22c{1} %-22.22M - %m%n"/>
      </layout>
    </appender>
    <root>
      <appender-ref ref="RollingLogFileAppender"/>
      <level value="DEBUG"/>
    </root>
  </log4net>
  <runtime>
    <legacyCorruptedStateExceptionsPolicy enabled="true"/>
  </runtime>
  <appSettings>
    <add key="ExtensionsForScan" value="7z,386,3GR,ADD,ANI,ASP,ASPX,ASX,BAT,CAB,CHM,CLASS,CMD,COM,CPL,DBX,DEX,DLL,DMD,DOC,DOCM,DOCX,DOT,DRV,EML,EXE,FLT,FON,HLP,HT?,HTA,I13,IFS,INF,INI,JAR,JPEG,JPG,JPZ,JS,LNK,MOD,MPD,MPP,MPT,MSO,MUI,NWS,OCX,OV?,PDF,PDR,PHP,PIF,PL,POT,PPS,PPT,PPTM,PPTX,PRC,RTF,SCR,SH,SHB,SHS,SRC,SWF,SYS,VB?,VXD,WBK,WMA,WMF,WSF,XL?,XLSM,XLSX,XSN,ZIP,ZIPX"/>
    <add key="ScanAll" value="False"/>
    <add key="ClientSettingsProvider.ServiceUri" value=""/>
    <add key="UseCachedVirtualSystemManagementService" value="false"/>
    <add key="SummaryInformationRequestTimeout" value="60"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="NetTcpBinding_IManagement" closeTimeout="12:00:00" openTimeout="12:00:00" receiveTimeout="01:00:00" sendTimeout="12:00:00" transactionFlow="false" transferMode="Buffered" transactionProtocol="OleTransactions" hostNameComparisonMode="StrongWildcard" listenBacklog="10" maxBufferPoolSize="524288" maxBufferSize="33554432" maxConnections="10" maxReceivedMessageSize="33554432">
          <readerQuotas maxDepth="32" maxStringContentLength="33554432" maxArrayLength="524288" maxBytesPerRead="65536" maxNameTableCharCount="16384"/>
          <reliableSession ordered="true" inactivityTimeout="00:10:00" enabled="false"/>
          <security mode="None"/>
        </binding>
      </netTcpBinding>
      <wsHttpBinding>
        <binding name="WSHttpBinding_IVersionService">
          <security mode="None"/>
        </binding>
      </wsHttpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://localhost:8535/FiveNine.Antivirus.ManagementService" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IManagement" contract="ManagementServiceReference.IManagement" name="NetTcpBinding_IManagement">
        <identity>
          <dns value="localhost"/>
        </identity>
      </endpoint>
      <endpoint address="http://www.5nine.com/UpdateCheck/VersionService.svc" binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IVersionService" contract="CheckUpdateService.IVersionService" name="WSHttpBinding_IVersionService"/>
    </client>
  </system.serviceModel>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri=""/>
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400"/>
      </providers>
    </roleManager>
  </system.web>
</configuration>
